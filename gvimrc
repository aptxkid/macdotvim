set guifont=Menlo:h14

filetype on
filetype indent on
filetype plugin on
set laststatus=2
syntax on

set syntax=on
set autoindent
set cindent
set tabstop=4
set shiftwidth=4
:colorscheme wombat

hi Cursor guibg=darkred ctermbg=darkred
hi CursorLine   cterm=NONE ctermbg=darkblue guibg=darkblue
set cursorline
